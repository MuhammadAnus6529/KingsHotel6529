{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/(register|login|rooms|bookings|my-bookings|admin/.*|check-email)",
      "dest": "app.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "Frontend/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}